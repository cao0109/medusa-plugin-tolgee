{
  "name": "@hicity/medusa-plugin-tolgee",
  "description": "Medusa V2 Plugin to easily add translations to your models with Tolgee Integration",
  "author": "Allen Cao",
  "version": "1.3.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cao0109/medusa-plugin-tolgee"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    ".medusa/server"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    ".": "./.medusa/server/src/modules/tolgee/index.js",
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "keywords": [
    "medusa-plugin-integration",
    "translation",
    "tolgee",
    "ecommerce",
    "medusa-plugin-cms",
    "medusa-v2",
    "medusa",
    "medusa-plugin-tolgee",
    "medusa-plugin-tolgee-v2",
    "medusa-plugin-tolgee-v2-integration"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "medusa plugin:develop",
    "prepublishOnly": "medusa plugin:build"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "2.8.2",
    "@medusajs/cli": "2.8.2",
    "@medusajs/framework": "2.8.2",
    "@medusajs/icons": "2.8.2",
    "@medusajs/js-sdk": "2.8.2",
    "@medusajs/medusa": "2.8.2",
    "@medusajs/test-utils": "2.8.2",
    "@medusajs/ui": "4.0.12",
    "@medusajs/utils": "2.8.2",
    "@mikro-orm/cli": "6.4.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@swc/core": "1.5.7",
    "@tanstack/react-query": "5.64.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "awilix": "^8.0.1",
    "pg": "^8.13.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.20.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "^2.7.0",
    "@medusajs/cli": "^2.7.0",
    "@medusajs/framework": "^2.7.0",
    "@medusajs/icons": "^2.7.0",
    "@medusajs/medusa": "^2.7.0",
    "@medusajs/test-utils": "^2.7.0",
    "@medusajs/ui": "^4.0.8",
    "@mikro-orm/cli": "6.4.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "awilix": "^8.0.1",
    "pg": "^8.13.0",
    "react-i18next": "^13.5.0"
  },
  "dependencies": {
    "@tolgee/core": "^6.2.2",
    "@tolgee/react": "^6.2.2",
    "@tolgee/web": "^6.2.2",
    "axios": "1.7.8",
    "axios-cache-interceptor": "1.8.0"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.1.2"
}
